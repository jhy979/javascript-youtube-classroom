{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "plugin:import/recommended","prettier"],
  "plugins": ["prettier", "import"],
	"parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
	"no-unused-vars": "warn"
  }
}
